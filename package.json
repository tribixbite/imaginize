{
  "name": "illustrate",
  "version": "1.0.0",
  "description": "AI-powered book illustration guide generator - extracts key visual concepts from EPUB and PDF books",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "illustrate": "./bin/illustrate.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "node --test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "epub",
    "pdf",
    "book",
    "illustration",
    "ai",
    "openai",
    "gpt",
    "image-generation",
    "ebook",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/illustrate.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/illustrate/issues"
  },
  "homepage": "https://github.com/yourusername/illustrate#readme",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "xml2js": "^0.6.2",
    "pdf-parse": "^1.1.1",
    "openai": "^4.63.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "cheerio": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/pdf-parse": "^1.1.4",
    "typescript": "^5.4.5",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ]
}
