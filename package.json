{
  "name": "imaginize",
  "version": "2.6.2",
  "type": "module",
  "description": "AI-powered book illustration guide generator with real-time web dashboard - Visual style consistency, parallel batch processing, character tracking, and optimized performance with free OpenRouter support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "imaginize": "bin/imaginize.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "check-updates": "npm outdated || true",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "epub",
    "pdf",
    "book",
    "illustration",
    "ai",
    "openai",
    "gpt",
    "image-generation",
    "ebook",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tribixbite/imaginize.git"
  },
  "bugs": {
    "url": "https://github.com/tribixbite/imaginize/issues"
  },
  "homepage": "https://github.com/tribixbite/imaginize#readme",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.63.0",
    "ora": "^8.1.0",
    "pdf-parse": "^1.1.1",
    "ws": "^8.14.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/ws": "^8.5.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ]
}
